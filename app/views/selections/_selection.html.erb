<div class="col mb-5 ">
  <h4 class="mb-3"><%= selection.title %></h4>
  <ul class="list-group list-group-flush px-3">
    <% selection.names.each do |id, name| %>
      <li class="list-group-item plain-text"> > <%= link_to name, name_path(id), class: "text-dark" %></li>
    <% end %>
  </ul>
</div>
